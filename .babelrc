{
  "presets": [
    ["env", {
      "production": {
        "targets": {
          "browsers": ["last 5 versions", "safari >= 7"]
        },
        "modules": false,
      },
      "development": {
        "targets": {
          "browsers": ["last 5 versions", "safari >= 7"]
        },
        "modules": false,
      },
      "test": {
        "plugins": ["transform-es2015-modules-commonjs"],
        "modules": false,
      }
    }]
  ],
  "plugins": ["transform-object-rest-spread"]
}
